# vim: et ts=8 sts=2 sw=2
---
classes:
  MyClass:
    methods:
      # Note: no overloading
      bar: # by default, void bar();
      baz:
        return: int
        paramorder: [x, y, z]
        params:
          x: int
          y: str
          z: float
    staticmethods:
      g:
        return: double
functions:
  myFunc:
  myFunc2:
    params: # only 1, so paramorder not needed
      p1: int

